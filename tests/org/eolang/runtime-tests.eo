+package org.eolang
+junit

[] > understands-this-correctly
  [x] > a
    $.x > @
  eq. > @
    a 42
    42

[] > takes-parent-object
  [x] > a
    [] > take
      ^.x > @
    take > @
  eq. > @
    a 42
    42

[] > makes-object-a-constant
  [] > foo
    as-int. > @
      mul.
        random
        100.0
  foo.@ > f!
  eq. > @
    f
    f

[] > kids-are-accessible-in-const-object
  [] > a
    [] > hello
      42 > @
  a > b!
  eq. > @
    42
    b.hello

[] > takes-parent-through-attribute
  42 > x
  $ > this
  [] > @
    [] > @
      [] > @
        eq. > @
          42
          this.x

[] > throws-when-applies-to-closed-object
  [x] > a
    x > @
  a FALSE > closed
  closed TRUE > @
